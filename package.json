{
  "name": "greeneetwork",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "yarn build:sitemap && next build",
    "build:sitemap": "babel-node -x \".ts\" -r dotenv/config ./utils/index.ts",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "export": "yarn build && next export",
    "start": "next start",
    "pretty:quick": "pretty-quick --pattern '**/*.*(ts|tsx|jsx)' --staged",
    "pretty:write": "prettier \"{data,src,public,utils}/**/*.{ts,tsx,js,jsx,json,md,css,geojson}\" --write",
    "pretty:test": "prettier \"{data,src,public,utils}/**/*.{ts,tsx,js,jsx,json,md,css,geojson}\" --list-different",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "sb:test": "cd .storybook && jest storyShot.test.js && cd ..",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "type-check": "tsc"
  },
  "types": "/src/@types",
  "husky": {
    "hooks": {
      "pre-commit": "git add . && yarn pretty:quick",
      "pre-push": "yarn type-check && yarn test --passWithNoTests"
    }
  },
  "dependencies": {
    "@geist-ui/react": "^2.1.0-canary.0",
    "@geist-ui/react-icons": "^1.0.0",
    "@turf/centroid": "^6.0.2",
    "classnames": "^2.2.6",
    "d3-ease": "^2.0.0",
    "framer-motion": "^3.7.0",
    "gsap": "^3.5.1",
    "inter-ui": "^3.15.0",
    "next": "^10.0.7",
    "next-pwa": "^3.1.5",
    "react": "^17.0.1",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^17.0.1",
    "react-html-parser": "^2.0.2",
    "react-map-gl": "^5.2.9",
    "react-masonry-css": "^1.0.14",
    "react-owl-carousel": "^2.3.3",
    "react-player": "^2.6.2",
    "react-share": "^4.4.0",
    "use-supercluster": "^0.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@directus/sdk-js": "^6.3.0",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@testing-library/react": "^11.1.0",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.11.11",
    "@types/react": "^16.9.51",
    "babel-loader": "^8.1.0",
    "geojson": "^0.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.27.0",
    "sitemap": "^6.3.2",
    "supercluster": "^7.1.0",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3"
  }
}